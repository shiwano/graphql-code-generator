{
  "name": "@graphql-codegen/typescript-mongodb",
  "version": "2.1.1",
  "description": "GraphQL Code Generator plugin for generting a ready-to-use ORM types for MongoDB",
  "peerDependencies": {
    "graphql": "^0.8.0 || ^0.9.0 || ^0.10.0 || ^0.11.0 || ^0.12.0 || ^0.13.0 || ^14.0.0 || ^15.0.0",
    "graphql-tag": "^2.0.0"
  },
  "dependencies": {
    "@graphql-codegen/plugin-helpers": "^2.1.0",
    "@graphql-codegen/typescript": "^2.1.1",
    "@graphql-codegen/visitor-plugin-common": "2.1.1",
    "@graphql-tools/utils": "^8.1.1",
    "auto-bind": "~4.0.0",
    "lodash": "~4.17.0",
    "tslib": "~2.3.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dotansimha/graphql-code-generator.git",
    "directory": "packages/plugins/typescript/mongodb"
  },
  "license": "MIT",
  "main": "index.js",
  "module": "index.mjs",
  "typings": "index.d.ts",
  "typescript": {
    "definition": "index.d.ts"
  },
  "exports": {
    ".": {
      "require": "./index.js",
      "import": "./index.mjs"
    },
    "./*": {
      "require": "./*.js",
      "import": "./*.mjs"
    }
  }
}
